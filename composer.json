{
    "name": "davidarl/wa-fiture",
    "description": "PackStarter untuk Fitur WA MSD",
    "keywords": [
        "DavidArl",
        "laravel",
        "wa-fiture"
    ],
    "homepage": "https://github.com/davidarl/wa-fiture",
    "license": "MIT",
    "authors": [
        {
            "name": "David Aprilio",
            "email": "david.14pril@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/contracts": "^9.0",
        "spatie/laravel-package-tools": "^1.9.2",
        "yajra/laravel-datatables-oracle": "^10.1"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^6.0",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "DavidArl\\WaFiture\\": "src",
            "DavidArl\\WaFiture\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DavidArl\\WaFiture\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": false
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DavidArl\\WaFiture\\WaFitureServiceProvider"
            ],
            "aliases": {
                "WaFiture": "DavidArl\\WaFiture\\Facades\\WaFiture"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}